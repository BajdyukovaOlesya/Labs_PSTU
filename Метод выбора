#include <iostream>
#include <clocale>
using namespace std;

int main()
{
	int mas[7];
	for (int i = 0; i < 7; i++)  cin >> mas[i]; 

	int min;
	int y;
	for (int i = 0; i < 7; i++) {
		min = i;
		for (int j = i+1; j < 7; j++) {
			if (mas[j] < mas[min]) min = j;
		}
	y = mas[i];
		mas[i] = mas[min];
		mas[min] = y;
		}
	for (int i = 0; i < 7; i++)
		cout<< mas[i];
	return 0;
}
