#include <iostream>
#include <clocale>
using namespace std;
int str(int m[]){
	     cout << '(';
		 for (int j = 0; j < 12; j++) {
				 if (m[0]!=0) {
					 if ((j != 11) && (m[j] == 0)) cout << ','; else { if (m[j] != 0) cout << m[j]; }
				 }
				 else { if ((j==0)&&(m[j]==0)) j += 1;
				 if (m[j] == 0) cout << ','; else cout << m[j];
				 }
		 }
		cout << ") ";
		return 0;
	}
int main()
{
	int mas[4][12] = { 1,2,7,0,7,8,5,4,9,6,3,1,0,4,7,0,3,0,1,0,8,4,5,6,3,1,2,0,8,4,5,6,0,7,0,4,5,9,5,7,9,0,4,3,9,7,3,0 };
	for (int i = 0; i < 4; i++) {
		for (int j = 0; j < 12; j++)
			cout << mas[i][j];
		cout << endl;
	}
	for (int i = 0; i < 4; i++) {
		str(mas[i]);
	}
		return 0;
	}
	
